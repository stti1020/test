    <!-- Wird beim entwickeln eingebunden-->
    <environment names="Development">
        <link rel="stylesheet" href="~/lib/$lib_bower/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/lib/$lib_bower/font-awesome/css/font-awesome.css" />
        <link rel="stylesheet" href="~/lib/$lib_bower/metisMenu/dist/metisMenu.css" />
        <link href="~/lib/$lib_bower/bootstrap-datepicker/dist/css/bootstrap-datepicker.css" rel="stylesheet"/>
        <link href="~/lib/$lib_bower/sweetalert/dist/sweetalert.css" rel="stylesheet" />
        <link href="~/lib/$lib_bower/angular-image-crop/image-crop-styles.css" rel="stylesheet" />
        <link rel="stylesheet" href="~/styles/css/style.css" asp-append-version="true" />

        <!--GoogleMapsApi-->
        <script src="http://maps.google.com/maps/api/js?libraries=places&sensor=false" type="text/javascript"></script>
    </environment>

    <!-- Wird beim deployment eingebunden-->
    <environment names="Staging,Production">
        <link rel="stylesheet" href="~/lib/$lib_bower/bootstrap/dist/css/bootstrap.min.css"/>
        <link rel="stylesheet" href="~/lib/$lib_bower/font-awesome/css/font-awesome.min.css"/>
        <link rel="stylesheet" href="~/lib/$lib_bower/metisMenu/dist/metisMenu.min.css"/>
        <link href="~/lib/$lib_bower/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css" rel="stylesheet" />
        <link href="~/lib/$lib_bower/sweetalert/dist/sweetalert.css" rel="stylesheet" />
        <link href="~/lib/$lib_bower/angular-image-crop/image-crop-styles.css" rel="stylesheet"/>
        <link rel="stylesheet" href="~/styles/css/style.css" asp-append-version="true"/>

        <!--GoogleMapsApi-->
        <script src="http://maps.google.com/maps/api/js?libraries=places&sensor=false" type="text/javascript"></script>
    </environment>

</head>
<body ng-app="app">

<!-- Linke Navigation -->
<div ng-include="'app/components/Navigation/navigation.html'"></div>
<div id="page-wrapper" class="gray-bg">
    <!-- Top Navigation -->
    <div ng-include="'app/components/Navigation/header.html'"></div>
    <!-- Inhalt -->
    <div id="maindiv" class="">
        <div ng-view></div>
    </div>
    <!--Footer-->
    <div ng-include="'app/components/Navigation/footer.html'"></div>
</div>

<!--Wird beim entwickeln eingebunden-->
